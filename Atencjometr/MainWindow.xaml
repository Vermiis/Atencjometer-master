        <Image x:Name="avm2" HorizontalAlignment="Left" Height="150" Margin="310,320,0,0" VerticalAlignment="Top" Width="150">
            <Image.Effect>
                <BlurEffect/>
            </Image.Effect>
          
        </Image>
        <Label x:Name="label" Content="Wspolne tagi" HorizontalAlignment="Left" Margin="220,76,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbl_wspolne" Content="Label" HorizontalAlignment="Left" Margin="220,182,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btn_compare" Content="Porownaj" HorizontalAlignment="Left" Margin="220,260,0,0" VerticalAlignment="Top" Width="75" Click="btn_compare_Click"/>

    </Grid>
</Window>
